.page-index{
    width: 98%;
    margin: 0 auto;
    height: -webkit-fill-available;
    background-color: #f0f0f0;
    text-align: center;
    padding-top: 2px;
}
.page-index .action{
  margin-top: 10px;
}

.page-index .action-cycle{
  margin-top: 50px;
}

.page-index .action-cycle button{
  width: 150px;
  height: 150px;
  line-height: 150px;
  vertical-align: middle;
  border-radius: 75px;
}